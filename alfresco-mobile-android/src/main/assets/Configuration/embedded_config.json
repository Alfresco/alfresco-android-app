  "evaluators": {
    "isAlfrescoV42": {
      "match-any": [
        "isAlfrescoV42-EE",
        "isAlfrescoV42-CE"
      ]
    },
    "isOnPremise": {
      "type": "org.alfresco.client.evaluator.hasRepositoryCapability",
      "params": {
        "session": "onpremise"
      }
    },
    "isAlfrescoV42-EE": {
      "type": "org.alfresco.client.evaluator.hasRepositoryCapability",
      "params": {
        "operator": ">=",
        "edition": "Enterprise",
        "majorVersion": 4,
        "minorVersion": 2
      }
    },
    "isAlfrescoV42-CE": {
      "type": "org.alfresco.client.evaluator.hasRepositoryCapability",
      "params": {
        "operator": ">=",
        "edition": "Community",
        "majorVersion": 4,
        "minorVersion": 2,
        "maintenanceVersion": "e"
      }
    },
    "hasAuthorAspect": {
      "type": "org.alfresco.client.evaluator.hasAspect",
      "params": {
        "aspectName": "cm:author"
      }
    },
    "hasTitledAspect": {
      "type": "org.alfresco.client.evaluator.hasAspect",
      "params": {
        "aspectName": "cm:titled"
      }
    }
  }
