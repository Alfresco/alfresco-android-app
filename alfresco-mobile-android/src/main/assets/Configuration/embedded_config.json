    "validation-mandatory": {
      "type": "org.alfresco.client.validation.mandatory",
      "error-id": "org.alfresco.client.validation.mandatory.error"
    },
    "validation-stringLength": {
      "type": "org.alfresco.client.validation.stringLength",
      "error-id": "The value must be between %s and %s characters long.",
      "params": {
        "min": 5,
        "max": 10
      }
    },
    "validation-numberRange": {
      "type": "org.alfresco.client.validation.numberRange",
      "error-id": "org.alfresco.client.validation.numberRange.error",
      "params": {
        "min": -100,
        "max": 250
      }
    },
    "validation-nodeName": {
      "type": "org.alfresco.client.validation.nodeName",
      "error-id": "org.alfresco.client.validation.nodeName.error"
    },
    "validation-regex-number": {
      "type": "org.alfresco.client.validation.regex",
      "error-id": "The value doesnt respect the pattern.",
      "params": {
        "pattern": "4{3}"
      }
    },
    "validation-regex-email": {
      "type": "org.alfresco.client.validation.regex",
      "error-id": "The value doesnt respect the pattern.",
      "params": {
        "pattern": "[A-Za-z0-9._]*+@[A-Za-z0-9.]*"
      }
    }
  },
  "evaluators": {
    "isAlfrescoV42": {
      "match-any": [
        "isAlfrescoV42-EE",
        "isAlfrescoV42-CE"
      ]
    },
    "isAlfrescoV42-EE": {
      "type": "org.alfresco.client.evaluator.isRepositoryVersion",
      "params": {
        "operator": ">=",
        "edition": "Enterprise",
        "majorVersion": 4,
        "minorVersion": 2
      }
    },
    "isAlfrescoV42-CE": {
      "type": "org.alfresco.client.evaluator.isRepositoryVersion",
      "params": {
        "operator": ">=",
        "edition": "Community",
        "majorVersion": 4,
        "minorVersion": 2,
        "maintenanceVersion": "e"
      }
    },
